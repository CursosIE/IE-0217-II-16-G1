#include <stdio.h>
#include <time.h>
#include <iostream>

using namespace std;

int main(int argc, char *argv[])
{
  clock_t t_ini, t_fin;
  double secs;

  t_ini = clock();
	cout << "Imprimiendo" << endl;
	cout << "Imprimiendo" << endl;
	cout << "Imprimiendo" << endl;
	cout << "Imprimiendo" << endl;
	cout << "Imprimiendo" << endl;
	cout << "Imprimiendo" << endl;
	cout << "Imprimiendo" << endl;
	cout << "Imprimiendo" << endl;
  t_fin = clock();

  secs = (double)(t_fin - t_ini) / CLOCKS_PER_SEC;
  printf("%.16g milisegundos\n", secs * 1000.0);
  return 0;
}